block fotorama {

    this.ctx.options {

        attrs: {
            var attrs   = {};
            var options = this.ctx.options;

            for (var i in options) {
                attrs['data-' + i] = options[i];
            }

            return attrs;
        }
    }

    elem img {

        tag: 'img'

        attrs: {

            src: this.ctx.url
        }
    }
}